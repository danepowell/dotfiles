{
    "title": "Dane Emacs everywhere",
    "rules": [
        {
            "description": "Copy / cut / paste",
            "manipulators": [
                {
                    "conditions": [
                        {
                            "bundle_identifiers": [
                                "^com\\.jetbrains\\.PhpStorm$",
                                "^com\\.googlecode\\.iterm2$",
                                "^org\\.gnu\\.Emacs$",
                                "^org\\.gnu\\.AquamacsEmacs$",
                                "^org\\.gnu\\.Aquamacs$",
                                "^org\\.pqrs\\.unknownapp.conkeror$",
                                "^com\\.microsoft\\.rdc$",
                                "^com\\.microsoft\\.rdc\\.",
                                "^net\\.sf\\.cord$",
                                "^com\\.thinomenon\\.RemoteDesktopConnection$",
                                "^com\\.itap-mobile\\.qmote$",
                                "^com\\.nulana\\.remotixmac$",
                                "^com\\.p5sys\\.jump\\.mac\\.viewer$",
                                "^com\\.p5sys\\.jump\\.mac\\.viewer\\.",
                                "^com\\.teamviewer\\.TeamViewer$",
                                "^com\\.vmware\\.horizon$",
                                "^com\\.2X\\.Client\\.Mac$",
                                "^com\\.apple\\.Terminal$",
                                "^co\\.zeit\\.hyperterm$",
                                "^co\\.zeit\\.hyper$",
                                "^io\\.alacritty$",
                                "^net\\.kovidgoyal\\.kitty$",
                                "^org\\.vim\\.",
                                "^com\\.qvacua\\.VimR$",
                                "^com\\.vmware\\.fusion$",
                                "^com\\.vmware\\.horizon$",
                                "^com\\.vmware\\.view$",
                                "^com\\.parallels\\.desktop$",
                                "^com\\.parallels\\.vm$",
                                "^com\\.parallels\\.desktop\\.console$",
                                "^org\\.virtualbox\\.app\\.VirtualBoxVM$",
                                "^com\\.citrix\\.XenAppViewer$",
                                "^com\\.vmware\\.proxyApp\\.",
                                "^com\\.parallels\\.winapp\\.",
                                "^org\\.x\\.X11$",
                                "^com\\.apple\\.x11$",
                                "^org\\.macosforge\\.xquartz\\.X11$",
                                "^org\\.macports\\.X11$",
                                "^com\\.sublimetext\\.",
                                "^com\\.microsoft\\.VSCode$"
                            ],
                            "type": "frontmost_application_unless"
                        }
                    ],
                    "from": {
                        "key_code": "y",
                        "modifiers": {
                            "mandatory": [
                                "control"
                            ],
                            "optional": [
                                "caps_lock"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "v",
                            "modifiers": [
                                "left_command"
                            ]
                        }
                    ],
                    "type": "basic"
                },
                {
                    "conditions": [
                        {
                            "bundle_identifiers": [
                                "^com\\.jetbrains\\.PhpStorm$",
                                "^com\\.googlecode\\.iterm2$",
                                "^org\\.gnu\\.Emacs$",
