                    ],
                    "from": {
                        "key_code": "w",
                        "modifiers": {
                            "mandatory": [
                                "control"
                            ],
                            "optional": [
                                "caps_lock"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "c",
                            "modifiers": [
                                "left_command"
                            ]
                        }
                    ],
                    "type": "basic"
                }
            ]
        }
    ]
}
